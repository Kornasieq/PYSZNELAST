<Window x:Class="PYSZNELAST.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PYSZNELAST"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <Button Content="Zamówienie" MinWidth="100" Margin="10,0,0,0" Click="Btn1"/>
            <Button Content="Użytkownik" MinWidth="100" Margin="10,0,0,0" Click="Btn2"/>
            <Button Content="Restauracja" MinWidth="100" Margin="10,0,0,0" Click="Btn3"/>

        </StackPanel>
        <Frame x:Name="MainW" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
